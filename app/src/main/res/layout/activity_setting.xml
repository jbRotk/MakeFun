<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.makefun.view.activity.SettingActivity"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/Topbar_hight"
        android:background="@color/colorPrimary">

        <ImageView
            android:layout_width="@dimen/Setting_top_back_size"
            android:layout_height="@dimen/Setting_top_back_size"
            android:id="@+id/setting_top_back"
            android:src="@drawable/topbar_back_icon"
            android:layout_marginLeft="8dp"
            android:layout_gravity="center_vertical" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SettingTitle"
            android:id="@+id/setting_title"
            android:layout_gravity="center_vertical"
            android:textColor="@color/TextColor"
            android:textSize="@dimen/LargerSize" />
    </LinearLayout>

    <com.example.administrator.makefun.view.widget.scrollview.OverScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/scrollView"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            android:paddingBottom="100dp"
            android:paddingTop="16dp">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设置"
                    android:id="@+id/textView"
                    android:textSize="18dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_line_width"
                    android:background="@color/Setting_line_color"
                    android:layout_marginTop="4dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/setting_context_bonderline"
                android:layout_marginTop="8dp" >

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:id="@+id/relativeLayout" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView19"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/AutomaticPlay"
                        android:id="@+id/textView2"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView19"
                        android:layout_toEndOf="@+id/imageView19"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView20"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/Setting_line_width"
                        android:background="@color/Setting_line_color"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:clickable="true"
                    android:background="?android:attr/selectableItemBackground"
                    android:id="@+id/relativeLayout2" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView21"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ClearCash"
                        android:id="@+id/textView3"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView21"
                        android:layout_toEndOf="@+id/imageView21"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView22"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="互动"
                    android:id="@+id/textView4"
                    android:textSize="18dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_line_width"
                    android:background="@color/Setting_line_color"
                    android:layout_marginTop="4dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/setting_context_bonderline"
                android:layout_marginTop="8dp" >

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:id="@+id/relativeLayout3" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView23"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/RecommendFriends"
                        android:id="@+id/textView5"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView23"
                        android:layout_toEndOf="@+id/imageView23"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView24"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/Setting_line_width"
                        android:background="@color/Setting_line_color"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:clickable="true"
                    android:background="?android:attr/selectableItemBackground"
                    android:id="@+id/relativeLayout4" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView25"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView26"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/HighOpinion"
                        android:id="@+id/textView6"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView25"
                        android:layout_toEndOf="@+id/imageView25"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/Setting_line_width"
                        android:background="@color/Setting_line_color"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:clickable="true"
                    android:background="?android:attr/selectableItemBackground"
                    android:id="@+id/relativeLayout5" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView27"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/FeedBack"
                        android:id="@+id/textView7"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView27"
                        android:layout_toEndOf="@+id/imageView27"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView28"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="关于我们"
                    android:id="@+id/textView8"
                    android:textSize="18dp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_line_width"
                    android:background="@color/Setting_line_color"
                    android:layout_marginTop="4dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/setting_context_bonderline"
                android:layout_marginTop="8dp">

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:background="?android:attr/selectableItemBackground"
                    android:clickable="true"
                    android:id="@+id/relativeLayout6" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView29"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView30"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/AboutUs"
                        android:id="@+id/textView9"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView29"
                        android:layout_toEndOf="@+id/imageView29"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/Setting_line_width"
                        android:background="@color/Setting_line_color"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />
                </RelativeLayout>

                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/Setting_item_high"
                    android:clickable="true"
                    android:background="?android:attr/selectableItemBackground"
                    android:id="@+id/relativeLayout7" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView31"
                        android:src="@android:drawable/btn_star_big_on"
                        android:layout_gravity="center_vertical"
                        android:layout_alignBottom="@+id/imageView32"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Donate"
                        android:id="@+id/textView10"
                        android:layout_gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/imageView31"
                        android:layout_toEndOf="@+id/imageView31"
                        android:layout_marginLeft="8dp"
                        android:textSize="@dimen/Setting_item_text_size"
                        android:textColor="@color/Setting_item_text_color" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/Setting_item_next_high"
                        android:id="@+id/imageView32"
                        android:src="@drawable/gray_next_icon"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginRight="8dp" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </com.example.administrator.makefun.view.widget.scrollview.OverScrollView>

</LinearLayout>
